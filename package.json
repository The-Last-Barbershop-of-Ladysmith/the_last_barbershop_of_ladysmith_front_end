{
  "name": "my-react-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "comigo-tech-react-input-mask": "^3.0.0-alpha.3",
    "create-react-app": "4.0.3",
    "framer-motion": "4.1.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npx cross-env CI=true npm run test:run",
    "test:run": "run-s test:e2e",
    "test:e2e": "run-p -l -r test:e2e:run test:start:* ",
    "test:e2e:run": "jest  --detectOpenHandles --forceExit --colors  --config ./e2e/jest.config.js || exit 0",
    "test:start:frontend": "npx cross-env NODE_ENV=test  react-scripts start ",
    "test:1": "run-p -l -r test:start:* \"test:e2e:run -- 01\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-fetch": "^3.0.6",
    "jest-puppeteer": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.4.0"
  }
}
